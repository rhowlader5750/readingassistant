{
  "manifest_version": 3,
  "name": "React Chrome ",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>", "file:///*"],
  "background": {
    "service_worker": "background.js"  
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_idle"
    }
  ]
}
